extends layout

block content
	body
		.contaier-fluid.gray-bg
			.container.mt-5
				.row.justify-content-center
					#postsContainer.col-6
						.card.mb-4
						img.card-img-top(src='img/homeCover.png')
						.card-body
							h5.card-title Post title
							p.card-text Post body
							a.btn.btn-primary(href='#') Go somewhere

					.col-3.d-none.d-lg-block
						.position-fixed
							.card
								.card-body
									h5.card-title About Catz
									.card-text
										ul
											li Epic cat social media
											li Privacy is non exsiting
											li We love catz
										p Links to epicness:
										ul
											li Profile
											li Search
									a.btn.btn-primary(href='#') Upload photo
